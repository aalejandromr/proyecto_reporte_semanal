<p id="notice"><%= notice %></p>

<h1>Asistencia Domingos</h1>

<table>
  <thead>
    <tr>
      <th>Hnos a</th>
      <th>Hnos i</th>
      <th>Friends</th>
      <th>Childrens</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @asistencia_domingos.each do |asistencia_domingo| %>
      <tr>
        <td><%= asistencia_domingo.hnos_a %></td>
        <td><%= asistencia_domingo.hnos_i %></td>
        <td><%= asistencia_domingo.friends %></td>
        <td><%= asistencia_domingo.childrens %></td>
        <td><%= link_to 'Show', asistencia_domingo %></td>
        <td><%= link_to 'Edit', edit_asistencia_domingo_path(asistencia_domingo) %></td>
        <td><%= link_to 'Destroy', asistencia_domingo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Asistencia Domingo', new_asistencia_domingo_path %>
